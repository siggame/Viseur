{
  "name": "Viseur",
  "version": "1.0.0",
  "description": "Visualizer for the Cadre AI game framework",
  "repository": {
    "type": "git",
    "url": "git://github.com/siggame/Viseur"
  },
  "scripts": {
    "visualizer": "webpack-dev-server --progress --colors --host 0.0.0.0",
