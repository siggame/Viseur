  "private": true,
  "dependencies": {
    "brfs": "^1.4.3 ",
    "cadre-js-extensions": "siggame/Cadre-js-extensions",
    "chess.js": "0.10.2",
    "classe": "1.0.0",
    "color": "0.11.4",
    "css-loader": "^0.26.1",
    "dateformat": "2.0.0",
    "eases": "1.0.8",
    "file-loader": "^0.10.0",
    "filesaverjs": "1.2.0",
    "font-awesome": "4.7.0",
    "font-awesome-webpack": "^0.0.4",
    "handlebars": "4.0.6",
    "handlebars-loader": "^1.3.0",
    "html-loader": "^0.4.3",
    "humps": "2.0.0",
    "less": "2.7.2",
    "image-webpack-loader": "^3.2.0",
    "json-loader": "^0.5.4",
    "jquery": "2.2.3",
    "node-sass": "4.5.0",
    "pixi.js": "4.3.5",
    "query-string": "4.3.1",
    "sass-loader": "^5.0.1",
    "store": "1.3.20",
    "style-loader": "^0.13.1",
    "transform-loader": "^0.2.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "eslint": "^3.7.1",
    "ghooks": "^2.0.0",
    "webpack-dev-server": "^1.15.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "eslint --cache ./",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "license": "MIT"
}
